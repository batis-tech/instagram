<%= form_for @post , html: {multiple: true} do |f| %>
<div class="form-group">
<%= f.file_field :image %>
<%= f.hidden_field :image_cache %>
</div>
<div class="form-group">
<%= f.label :description %>
<%= f.text_area :description,class:"form-control" %>
</div>
<%= f.submit "Upload",class:"btn btn-success"  %>

<% end %>
